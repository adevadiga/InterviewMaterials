package com.hackersrank.pgm;

import java.util.Scanner;

public class TestRune {

	public static void main(String[] args){
		//TimeToText.convert();
		 Scanner in = new Scanner(System.in);
			int t = in.nextInt();
	        int n = 0;
			for(int i=0; i<t;i++){
	            n = in.nextInt();
	            int res = (int)Math.pow(2, (n+1)) - 1;
	        }
			
	}
}
