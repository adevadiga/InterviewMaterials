package com.cci.questions.recursion;

import java.util.ArrayList;
import java.util.Arrays;

import org.junit.Test;

public class TestPrintAllSubsetOfSet {

	@Test
	public void testGetSubSets(){
		List<Integer> set = (ArrayList<Integer>) Arrays.asList(new Integer[]{1,2,3});
		ArrayList<ArrayList<Integer>> allSubSets = new PrintAllSubsetOfSet().getSubSets(set);
		for(ArrayList<Integer> subSet:allSubSets){
		   System.out.println(subSet.toString());
		}
	}
}
