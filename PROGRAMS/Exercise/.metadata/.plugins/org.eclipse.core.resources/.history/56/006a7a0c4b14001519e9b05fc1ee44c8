package com.cci.questions.StacksAndQueues.StackofStacks;

public class Stack {
	Node top;
	Node below;
	int capacity;
	int size;
	
	public Statck(int capacity){
		this.capacity = capacity;
	}
	public boolean isAtCapacity(){
		
	}
	
	public void join(Node above, Node below){
		if(below != null){
			below.above = above;
		}
		if(above != null){
			above.below = below;
		}
	}
	
	public boolean push(){
		
	}

}
