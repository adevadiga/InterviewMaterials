package com.company.jpmorgan;

class Node{
	int data;
	Node previous;
	Node next;
	Node(int data){
		this.data = data;
	}
	
	void appendToTail(int data){
		Node t = new Node(data);
		Node n = this;
		while(n.next != null){
			n = n.next;
		}
		n.next = t;
		t.previous = n;
		
	}
	
