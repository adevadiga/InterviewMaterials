package com.company.jpmorgan;

public class SpiralPrintNbyNMatrixForN {

	public static void main(String[] args){
		
	}
	
	public static void spiral(int n){
		int dx = 0,dy=0;
		int max = n*n;
		int counter=1;
		int[][] matrix = new int[n][n];
		int x=0,y=0;
		
		while(counter <= max){
			n[x][y] = counter++;
			
		}
	}
}
